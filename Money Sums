#include<bits/stdc++.h>  
using namespace std;  
main()
{   
    int t,i,j,n;  
    t=1;  
    while(t--)
    {    
        cin>>n;  
        int a[n+1],sum=0,cnt=0;
        for(i=0;i<n;i++)
        {  
            cin>>a[i];    
            sum+=a[i];
        }      
        bool dp[n+1][sum+1];    
        for(i=0;i<=n;i++)
        {  
            dp[i][0]=true;  
        }  
        for(i=1;i<=sum;i++)
        {  
            dp[0][i]=false;   
        }
        for(i=1;i<=n;i++)
        {  
            for(j=1;j<=sum;j++)
            {  
                if(a[i-1]<=j)
                {  
                    dp[i][j]=dp[i-1][j-a[i-1]] || dp[i-1][j];   
                }  
                else 
                {  
                    dp[i][j]=dp[i-1][j];  
                }  
            }  
        }  
        cnt=0;  
        for(i=1;i<=sum;i++)
        {  
           if(dp[n][i])
           {  
               cnt+=1;  
           }
        }  
        cout<<cnt<<"\n";  
        for(i=1;i<=sum;i++)
        {  
            if(dp[n][i])
            {  
                cout<<i<<" ";  
            }  
        }
    }  
}
