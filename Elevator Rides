#include <iostream>
#include<bits/stdc++.h>
using namespace std;
main()
{  
    long int n,x,i,j;
    cin>>n>>x;
    long int a[n+1];
    for(i=0;i<n;i++) 
    {
        cin>>a[i];
    } 
    pair<long int,long int> dp[1<<n];
    dp[0]={1,0}; 
    for(i=1;i<(1<<n);i++)
    { 
        dp[i]={21,0};
        for(j=0;j<n;j++)
        { 
            if(i&(1<<j))
            {
                auto opt=dp[i^(1<<j)];
                if(opt.second+a[j]<=x)
                {
                    opt.second+=a[j];
                }
                else
                {
                    opt.first+=1;
                    opt.second=a[j];
                }
                dp[i]=min(dp[i],opt); 
            }
        }
    } 
    cout<<dp[(1<<n)-1].first;
    return 0;
}
