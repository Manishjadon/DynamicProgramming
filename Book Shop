#include<bits/stdc++.h>  
using namespace std;     
main()  
{  
        int i,j,n,price; 
        cin>>n>>price;  
        int page[n+1];  
        int cost[n+1]; 
        for(i=0;i<n;i++)
        {  
            cin>>cost[i];  
        }
        for(i=0;i<n;i++)
        {  
            cin>>page[i];  
        }  
        int dp[n+1][price+1];   
        for(i=0;i<=price;i++)
        {  
            dp[0][i]=0;  
        }  
        for(i=0;i<=n;i++)
        {  
            dp[i][0]=0;  
        }   
        for(i=1;i<=n;i++)
        {  
            for(j=1;j<=price;j++)
            {  
                if(cost[i-1]<=j)
                {  
                    dp[i][j]=max(page[i-1]+dp[i-1][j-cost[i-1]],dp[i-1][j]);  
                }  
                else 
                {  
                    dp[i][j]=dp[i-1][j];  
                }  
            }  
        }  
        cout<<dp[n][price];
}
