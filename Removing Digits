#include<bits/stdc++.h>  
using namespace std;     
int dp[1000001];
int solve(int n)
{  
    if(n==0)
    {  
        return 0;  
    }     
    if(dp[n]!=-1)
    {  
        return dp[n];  
    }
    int m=n;    
    vector<int> vp;
    while(m>0)  
    {   
        if(m%10!=0)
        {
        vp.push_back(m%10);    
        }
        m=m/10;   
    }   
    int ans,i;  
    ans=INT32_MAX-1;
    for(i=vp.size()-1;i>=0;i--)
    {  
        ans=min(ans,1+solve(n-vp[i]));       
    }   
    dp[n]=ans;
    return ans;
}
main()  
{  
        int i,j,n; 
        cin>>n;    
        memset(dp,-1,sizeof dp);
        cout<<solve(n);
}
